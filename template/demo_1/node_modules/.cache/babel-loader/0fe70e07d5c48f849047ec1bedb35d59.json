 <vue-justgage ref=\"g2\" id=\"g2\" class=\"gauge\" data-value=\"100\"></vue-justgage>\n    <button href=\"javascript:void(0);\" class=\"btn btn-success\" @click=\"refresh\" id=\"g2_refresh\">Random Refresh</button>\n  </section>\n</template>\n\n<script>\nimport Vue from 'vue';\nimport vueJustgage from 'vue-justgage';\nVue.use(vueJustgage);\nexport default {\n  data() {\n    return {\n      dflt: {\n        min: 0,\n        max: 200,\n        donut: true,\n        gaugeWidthScale: 0.6,\n        counter: true,\n        hideInnerShadow: true,\n      }\n    }\n  },\n  mounted() {\n  this.$refs.g2.draw({\n      id: 'g2',\n      value: 100,\n      title: 'refresh',\n      defaults: this.dflt\n    });\n  },\n  methods: {\n    refresh() {\n      this.$refs.g2.refresh(this.getRandomInt(0, 100));\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components/charts/justgage"}]}