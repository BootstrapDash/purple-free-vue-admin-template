{"remainingRequest":"/Users/jyothish/Documents/purple-free-vue/template/demo_1/node_modules/vue-loader/lib/index.js??vue-loader-options!/Users/jyothish/Documents/purple-free-vue/template/demo_1/src/pages/charts/chartist.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"/Users/jyothish/Documents/purple-free-vue/template/demo_1/src/pages/charts/chartist.vue","mtime":1605587475606},{"path":"/Users/jyothish/Documents/purple-free-vue/template/demo_1/node_modules/css-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/jyothish/Documents/purple-free-vue/template/demo_1/node_modules/vue-loader/lib/loaders/stylePostLoader.js","mtime":499162500000},{"path":"/Users/jyothish/Documents/purple-free-vue/template/demo_1/node_modules/postcss-loader/src/index.js","mtime":499162500000},{"path":"/Users/jyothish/Documents/purple-free-vue/template/demo_1/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/jyothish/Documents/purple-free-vue/template/demo_1/node_modules/vue-loader/lib/index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["chartist.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsKA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"chartist.vue","sourceRoot":"src/pages/charts","sourcesContent":["<template>\n  <section class=\"chartist\">\n    <div class=\"page-header\">\n      <h3 class=\"page-title\">\n        Chartist\n      </h3>\n      <nav aria-label=\"breadcrumb\">\n        <ol class=\"breadcrumb\">\n          <li class=\"breadcrumb-item\"><a href=\"javascript:void(0);\">Charts</a></li>\n          <li class=\"breadcrumb-item active\" aria-current=\"page\">Chartist</li>\n        </ol>\n      </nav>\n    </div>\n    <div class=\"row\">\n      <div class=\"col-lg-6 grid-margin stretch-card\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">Line chart</h4>\n            <chartist\n                ratio=\"ct-major-second\"\n                type=\"Line\"\n                :data=\"simplelinechartData\"\n                :options=\"simplelinechartOptions\" >\n            </chartist>\n          </div>\n        </div>\n      </div>\n      <div class=\"col-lg-6 grid-margin stretch-card\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">Stacked Bar chart</h4>\n            <chartist\n                ratio=\"ct-major-second\"\n                type=\"Bar\"\n                :data=\"stackedbarchartData\"\n                :options=\"stackedbarchartOptions\" id=\"stackedBar\">\n            </chartist>\n          </div>\n        </div>\n      </div>\n      <div class=\"col-lg-6 grid-margin stretch-card\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">Horizontal Bar chart</h4>\n            <chartist\n                ratio=\"ct-major-second\"\n                type=\"Bar\"\n                :data=\"horizontalbarchartData\"\n                :options=\"horizontalbarchartOptions\" id=\"horizontalBarChart\">\n            </chartist>\n          </div>\n        </div>\n      </div>\n      <div class=\"col-lg-6 grid-margin stretch-card\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">Pie chart</h4>\n            <chartist\n                ratio=\"ct-major-second\"\n                type=\"Pie\"\n                :data=\"piechartData\"\n                :options=\"piechartOptions\" >\n            </chartist>\n          </div>\n        </div>\n      </div>\n      <div class=\"col-lg-6 grid-margin stretch-card\">\n        <div class=\"card\">\n          <div class=\"card-body\">\n            <h4 class=\"card-title\">Donut</h4>\n            <chartist\n                ratio=\"ct-major-second\"\n                type=\"Pie\"\n                :data=\"donutchartData\"\n                :options=\"donutchartOptions\" >\n            </chartist>\n          </div>\n        </div>\n      </div>\n    </div>\n    \n  </section>\n</template>\n\n<script>\n  import Vue from 'vue';\n  Vue.use(require('vue-chartist'))\n\n  export default {\n    name: 'app',\n    data () {\n      return {\n        simplelinechartData: {\n            labels: ['Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday'],\n            series:[\n              [12, 9, 7, 8, 5], \n              [2, 1, 3, 7, 3], \n              [1, 3, 4, 5, 6]]\n        },\n        simplelinechartOptions: {\n            lineSmooth: false,\n            fullWidth: true,\n            axisY: {\n            onlyInteger: true,\n          },\n        },\n        stackedbarchartData: {\n            labels: ['Q1', 'Q2', 'Q3', 'Q4'],\n            series: [\n              ['800000', '1200000', '1400000', '1300000'],\n              ['200000', '400000', '500000', '300000'],\n              ['100000', '200000', '400000', '600000'],\n              ['400000', '600000', '200000', '0000']\n            ]\n        },\n        stackedbarchartOptions: {\n          stackBars: true,\n          axisY: {\n            labelInterpolationFnc: function(value) {\n              return (value / 1000) + 'k';\n            }\n          }\n        },\n        horizontalbarchartData: {\n            labels: ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun'],\n            series: [\n              [5, 4, 3, 7, 5, 10, 3],\n              [3, 2, 9, 5, 4, 6, 4],\n              [2, 6, 7, 1, 3, 5, 9],\n              [2, 6, 7, 1, 3, 5, 19],\n            ]\n        },\n        horizontalbarchartOptions: {\n          seriesBarDistance: 10,\n          reverseData: true,\n          horizontalBars: true,\n          axisY: {\n            offset: 20\n          }\n        },\n        piechartData: {\n            series: [50, 30, 20]\n        },\n        piechartOptions: {\n            labelInterpolationFnc: function(value) {\n              return (value) + '%';\n          }\n        },\n        donutchartData: {\n            labels: ['safari', 'chrome', 'explorer', 'firefox'],\n            series: [20, 40, 10, 30]\n        },\n        donutchartOptions: {\n          donut: true,\n          donutWidth: 60,\n          donutSolid: true,\n          startAngle: 270,\n          showLabel: true,\n        }\n      }\n    },\n    mounted () {}\n    \n  }\n</script>\n<style>\n  #stackedBar .ct-series-a .ct-bar,\n  #stackedBar .ct-series-b .ct-bar,\n  #stackedBar .ct-series-c .ct-bar,\n  #stackedBar .ct-series-d .ct-bar {\n      stroke-width: 30px;\n  }\n  #horizontalBarChart .ct-series-a .ct-bar,\n  #horizontalBarChart .ct-series-b .ct-bar,\n  #horizontalBarChart .ct-series-c .ct-bar,\n  #horizontalBarChart .ct-series-d .ct-bar {\n      stroke-width: 10px;\n  }\n  \n</style>\n"]}]}